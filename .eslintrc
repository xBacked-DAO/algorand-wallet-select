{
    "parser": "@babel/eslint-parser",
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "space-before-function-paren": [
            "error",
            "never"
        ],
        "semi": [
            "error",
            "never"
        ],
        "no-underscore-dangle": 0,
        "quotes": [
            "error",
            "single"
        ],
        "react/prop-types": [
            "off"
        ]
    }
}