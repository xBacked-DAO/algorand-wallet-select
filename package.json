{
  "name": "@xbacked-dao/algorand-wallet-select",
  "author": "xBacked DAO",
  "module": "dist/algorand-wallet-select.esm.js",
  "version": "0.1.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "Web3 Algorand wallet selector",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "homepage": "https://github.com/xBacked-DAO/algorand-wallet-select",
  "bugs": {
    "url": "https://github.com/xBacked-DAO/algorand-wallet-select/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "tailwind": "npx tailwindcss -i ./src/css/tailwind.css -o ./src/css/output.css",
    "build:tailwind": "NODE_ENV=production yarn run tailwind --minify",
    "watch:tailwind": "yarn run tailwind --watch",
    "watch:tsdx": "tsdx watch",
    "push": "npm run prepush && cd ./dist && npm publish",
    "prepush": "npm run compile && cp package.json ./dist && cp README.md ./dist && cp LICENSE ./dist",
    "start": "npm-run-all -p watch:tsdx watch:tailwind storybook",
    "build": "yarn build:tailwind && tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    "clean": "rm -r dist",
    "postinstall": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.2",
    "@randlabs/myalgo-connect": "^1.1.1",
    "@walletconnect/client": "^1.6.6",
    "@walletconnect/qrcode-modal": "^1.6.6",
    "tailwindcss": "^2.2.19"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addons": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.4",
    "babel-jest": "^27.5.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "husky": "^7.0.4",
    "import": "^0.0.6",
    "lint-staged": "^12.3.8",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "tailwindcss": "^3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,css,md,ts,tsx}": "prettier --write"
  }
}
